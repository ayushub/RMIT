<%@page contentType="text/html" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <title>Enterprise Login</title>
        <link href="resources/login_style.css" rel="stylesheet" type="text/css" />
    </head>
    <body>
        <div id="main">
        <div class="caption">Enterprise Login</div>
        <div id="icon">&nbsp;</div>
        <form action="LoginServlet" method="post" name="loginform">
            <table width="100%">
                <tr><td>User ID:</td><td> <input class="text" name="user_id" type="text"  /></td></tr>
                <tr><td>Password:</td><td> <input class="text" name="password" type="password" /></td></tr>
                <tr><td colspan="2" align="center"><input class="text" type="submit" name="submitBtn" value="Login" /></td></tr>
            </table>  
        </form>
        </div>
    </body>
</html>
